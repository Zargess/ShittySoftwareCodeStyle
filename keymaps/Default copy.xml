<keymap version="1" name="Default copy" parent="$default">
  <action id="ActivateFindToolWindow">
    <keyboard-shortcut first-keystroke="alt 9" />
  </action>
  <action id="ActivateGradleToolWindow">
    <keyboard-shortcut first-keystroke="alt 9" />
  </action>
  <action id="ActivateVersionControlToolWindow">
    <keyboard-shortcut first-keystroke="alt 3" />
  </action>
  <action id="Annotate">
    <keyboard-shortcut first-keystroke="alt a" />
  </action>
  <action id="Git.Fetch">
    <keyboard-shortcut first-keystroke="ctrl alt r" />
  </action>
  <action id="Git.Pull">
    <keyboard-shortcut first-keystroke="ctrl t" />
  </action>
  <action id="Git.Stash">
    <keyboard-shortcut first-keystroke="ctrl alt numpad4" />
  </action>
  <action id="Git.Unstash">
    <keyboard-shortcut first-keystroke="ctrl alt numpad5" />
  </action>
  <action id="Scala.ShowImplicits">
    <keyboard-shortcut first-keystroke="shift ctrl alt equals" />
    <keyboard-shortcut first-keystroke="shift ctrl alt add" />
  </action>
  <action id="ToggleBookmark" />
  <action id="ToggleDistractionFreeMode">
    <keyboard-shortcut first-keystroke="ctrl alt numpad2" />
  </action>
  <action id="ToggleFullScreen">
    <keyboard-shortcut first-keystroke="f11" />
    <keyboard-shortcut first-keystroke="ctrl alt numpad1" />
  </action>
  <action id="TogglePresentationMode">
    <keyboard-shortcut first-keystroke="ctrl alt numpad3" />
  </action>
  <action id="Vcs.UpdateProject" />
</keymap>